{
  "name": "marvel-sssr",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/cheerio": "^0.22.10",
    "@zeit/next-typescript": "^1.1.1",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "http-proxy-middleware": "^0.19.1",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "^1.9.0",
    "next": "^8.0.2-canary.3",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "serverless-http": "^1.6.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=dev npm-run-all --parallel start:app start:server",
    "start:app": "PORT=3000 node server.js",
    "start:server": "netlify-lambda serve functions",
    "build": "npm-run-all --parallel build:**",
    "build:functions": "tsc",
    "build:serverless": "BUILD=serverless next build && npm run movestuff",
    "build:static": "BUILD=static next build && next export && npm run movestatic",
    "movestuff": "mkdir -p .netlify/functions/pages && mv .next/serverless/pages/* .netlify/functions/pages/",
    "movestatic": "rm -rf dist && mkdir -p dist/_next && mv .next/* dist/_next && npm run moveout",
    "moveout": "rsync -a out/ dist/ && rm -rf out",
    "olddev": "next start",
    "oldstart:app": "next",
    "oldbuild:functions": "netlify-lambda build functions",
    "oldexport": "npm run build && next export"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.1.0",
    "@types/aws-lambda": "^8.10.19",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/morgan": "^1.7.35",
    "@types/next": "^8.0.1",
    "@types/node": "^11.9.3",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "netlify-lambda": "^1.4.1",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.9.4",
    "typescript": "^3.3.3"
  }
}
